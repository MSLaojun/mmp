{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "ENV": true
    },
    "rules": {
        "indent": 0,
        "linebreak-style": [
            "error", "unix"
        ],
        "semi": 0,
        "no-console": 0
    }
}
