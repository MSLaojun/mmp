{
  "name": "mmap",
  "version": "0.1.3",
  "description": "JavaScript UMD module to create mind maps",
  "keywords": [
    "mmap",
    "mindmap",
    "node",
    "branch",
    "map",
    "mind",
    "infovis"
  ],
  "homepage": "https://github.com/cedoor/mmap",
  "license": "MIT",
  "author": {
    "name": "Omar Desogus",
    "url": "http://omardesogus.com"
  },
  "main": "dist/mmap.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cedoor/mmap.git"
  },
  "scripts": {
    "start": "npm run prepublish && http-server",
    "dev": "rollup -c -w",
    "pretest": "rollup -c",
    "test": "mocha",
    "prepublish": "npm run test && uglifyjs build/mmap.js --comments -c -m -o build/mmap.min.js"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "3",
    "http-server": "0.9",
    "mocha": "3",
    "rollup": "0.41",
    "rollup-plugin-babel": "2",
    "rollup-plugin-eslint": "3",
    "rollup-plugin-json": "2",
    "rollup-watch": "3",
    "uglify-js": "2"
  },
  "dependencies": {
    "d3": "^4.4.1"
  },
  "eslintConfig": {
      "env": {
          "browser": true,
          "es6": true
      },
      "extends": "eslint:recommended",
      "parserOptions": {
          "sourceType": "module"
      },
      "rules": {
          "no-console": 0
      }
  }
}
